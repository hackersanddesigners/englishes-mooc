{
  "name": "englishes-mooc",
  "version": "1.0.0",
  "description": "temporary landing page",
  "main": "index.js",
  "scripts": {
    "start": "watchify index.js -o ../public/assets/js/bundle.js --debug --verbose -t sheetify -p [ css-extract -o ../public/assets/css/bundle.css ] index.js",
    "build": "browserify index.js -o ../public/assets/js/bundle.js -t yo-yoify -t  [ sheetify -u sheetify-cssnext ] -g es2040 -p [ css-extract -o ../public/assets/css/bundle.css ] -p tinyify"
  },
  "author": "Andr√© Fincato",
  "license": "ISC",
  "sheetify": {
    "transform": [
      [
        "sheetify-cssnext",
        {
          "sourcemap": false
        }
      ]
    ]
  },
  "devDependencies": {
    "browserify": "^16.5.2",
    "choo-devtools": "^2.5.1",
    "css-extract": "^1.3.1",
    "es2040": "^1.2.6",
    "sheetify": "^7.4.0",
    "sheetify-cssnext": "^2.0.1",
    "tinyify": "^2.5.2",
    "watchify": "^3.11.1",
    "yo-yoify": "^4.3.0"
  },
  "dependencies": {
    "@vimeo/player": "^2.15.0",
    "choo": "^6.13.3",
    "dayjs": "^1.10.4",
    "domador": "^2.4.4",
    "filepond": "^4.25.1",
    "gr8": "^3.1.5",
    "js-yaml": "^3.14.1",
    "markdown-it": "^8.4.2",
    "markdown-it-container": "^2.0.0",
    "markdown-it-implicit-figures": "^0.9.0",
    "markdown-it-replace-link": "^1.1.0",
    "megamark": "^3.3.0",
    "object-keys": "^1.1.1",
    "object-values": "^2.0.0",
    "recsst": "^1.1.2",
    "slugify": "^1.4.6",
    "woofmark": "^4.2.6",
    "xhr": "^2.6.0",
    "xtend": "^4.0.2"
  }
}
